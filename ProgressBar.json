{
    "id": "com.gr.sap.progressbar",
    "version": "1.0.0",
    "name": "Progress Bar",
    "description": "Progress Bar",
    "newInstancePrefix": "ProgressBar",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-gr-progressbar",
            "url": "https:\/\/cdn.jsdelivr.net\/gh\/giovrizzo99\/ProgressBarWidget@main\/ProgressBar.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "builder",
            "tag": "com-gr-progressbar-builder",
            "url": "https:\/\/cdn.jsdelivr.net\/gh\/giovrizzo99\/ProgressBarWidget@main\/ProgressBar_Builder.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "percentage": {
            "description": "Percentage",
            "type": "integer",
            "default": 50
        },
        "barColor": {
            "description": "Bar Color",
            "type": "string",
            "default": "#00243b"
        },
        "emptyBarColor": {
            "description": "Empty Bar Color",
            "type": "string",
            "default": "#a9b4be"
        }
    },
    "methods": {
        "setPercentage": {
            "description": "Set Percentage",
            "parameters": [
                {
                    "name": "percentage",
                    "type": "integer",
                    "description": "Percentage"
                }
            ],
            "body": "this.percentage = percentage;"
        },
        "getPercentage": {
            "returnType": "integer",
            "description": "Return Percentage",
            "body": "return this.percentage;"
        },
        "setBarColor": {
            "description": "Set Bar Color",
            "parameters": [
                {
                    "name": "barColor",
                    "type": "string",
                    "description": "Bar Color"
                }
            ],
            "body": "this.barColor = barColor;"
        },
        "getBarColor": {
            "returnType": "string",
            "description": "Return Bar Color",
            "body": "return this.barColor;"
        },
        "setEmptyBarColor": {
            "description": "Set Empty Bar Color",
            "parameters": [
                {
                    "name": "emptyBarColor",
                    "type": "string",
                    "description": "Empty Bar Color"
                }
            ],
            "body": "this.emptyBarColor = emptyBarColor;"
        },
        "getEmptyBarColor": {
            "returnType": "string",
            "description": "Return Empty Bar Color",
            "body": "return this.emptyBarColor;"
        }
    },
    "events": {
        "onClick": {
            "description": "User Clicked."
        }
    }
}
